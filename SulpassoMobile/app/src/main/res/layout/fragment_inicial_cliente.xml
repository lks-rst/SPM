<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    style="@style/rootView.fragdesenv"
    android:layout_height="match_parent">

    <FrameLayout style="@style/rootView.fragEmp.frame" >

        <ImageView
            android:id="@+id/tela_inicial_img_logo"
            style="@style/rootView.fragEmp.frame.imagens"
            android:contentDescription="@string/apresentacao_1"
            android:src="@drawable/btn_sulpasso" />
    </FrameLayout>

    <LinearLayout
        style="@style/rootView.fragEmp.frame"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tela_inicial_txt_emp_nome"
            style="@style/rootView.fragEmp.frame.textos.negrito"
            android:text="@string/apresentacao_1" />

        <TextView
            android:id="@+id/tela_inicial_txt_emp_sub"
            style="@style/rootView.fragEmp.frame.textos"
            android:text="@string/apresentacao_1" />

        <TextView
            android:id="@+id/tela_inicial_txt_emp_fone"
            style="@style/rootView.fragEmp.frame.textos"
            android:text="@string/apresentacao_1" />

        <TextView
            android:id="@+id/tela_inicial_txt_emp_mail"
            style="@style/rootView.fragEmp.frame.textos"
            android:text="@string/apresentacao_1" />

        <TextView
            android:id="@+id/tela_inicial_txt_emp_page"
            style="@style/rootView.fragEmp.frame.textos"
            android:text="@string/apresentacao_1" />
    </LinearLayout>

</LinearLayout>